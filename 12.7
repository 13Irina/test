money = int(input("Введите сумму, которую хотите вложить: "))
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
deposit = []
for i in per_cent:
    d = int((money/100) * per_cent[i])
    deposit.append(d)
print(deposit)
print("Максимальная сумма, которую вы можете заработать: ", max(deposit))
